{
  "name": "headless-woo-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "cd apps/web && npm run dev",
    "dev:web": "cd apps/web && npm run dev",
    "dev:mobile": "cd apps/mobile && npm run start",
    "build": "cd apps/web && npm run build",
    "build:web": "cd apps/web && npm run build",
    "build:mobile": "cd apps/mobile && npm run build",
    "start": "cd apps/web && npm run start",
    "start:web": "cd apps/web && npm run start",
    "start:mobile": "cd apps/mobile && npm run start",
    "lint": "cd apps/web && npm run lint",
    "lint:fix": "cd apps/web && npm run lint:fix",
    "type-check": "cd apps/web && npm run type-check",
    "test": "cd apps/web && npm run test",
    "test:watch": "cd apps/web && npm run test:watch",
    "test:coverage": "cd apps/web && npm run test:coverage",
    "analyze": "cd apps/web && npm run analyze",
    "lighthouse": "cd apps/web && npm run lighthouse",
    "format": "cd apps/web && npm run format",
    "format:check": "cd apps/web && npm run format:check",
    "dev:debug": "cd apps/web && npm run dev:debug",
    "install:all": "npm install && cd apps/web && npm install && cd ../mobile && npm install && cd ../../packages/shared && npm install"
  },
  "dependencies": {
    "next": "15.5.6"
  },
  "pnpm": {
    "overrides": {
      "eslint-plugin-react-hooks": "^5.2.0",
      "send@<0.19.0": ">=0.19.0",
      "semver@>=7.0.0 <7.5.2": ">=7.5.2",
      "cookie@<0.7.0": ">=0.7.0",
      "tar@=7.5.1": ">=7.5.2",
      "@react-native-community/cli": "17.0.1",
      "@react-native-community/cli-doctor": "17.0.1",
      "@react-native-community/cli-platform-android": "17.0.1",
      "@react-native-community/cli-platform-ios": "17.0.1",
      "@react-native-community/cli-tools": "17.0.1"
    }
  }
}
